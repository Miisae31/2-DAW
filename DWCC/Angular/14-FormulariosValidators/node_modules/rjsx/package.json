{
  "name": "rjsx",
  "version": "1.0.1",
  "type": "module",
  "description": "一个极简的试图响应式库",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "build": "npm run clean && tsup",
    "minibuild": "npm run clean && tsup --minify",
    "npm:patch": "npm version patch",
    "npm:publish": "npm publish"
  },
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/typing.d.ts",
    "./jsx-runtime": "./dist/runtime.js",
    "./jsx-dev-runtime": "./dist/runtime.js"
  },
  "keywords": [
    "jsx",
    "runtime",
    "tsx"
  ],
  "author": "ameegle",
  "devDependencies": {
    "@types/node": "^20.13.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist"
  ]
}